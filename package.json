{
  "name": "tidalclone",
  "version": "1.0.0",
  "description": "CLI to clone Tidal accounts",
  "main": "app.js",
  "author": "Khaitu",
  "license": "MIT",
  "engines": {
    "node": ">=9.4"
  },
  "dependencies": {
    "minimist": "^1.2.0",
    "node-fetch": "^2.1.1",
    "terminal-kit": "^1.15.0"
  },
  "scripts": {
    "tidalclone": "node app"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "airbnb",
    "rules": {
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "consistent-return": "off",
      "arrow-parens": "off",
      "newline-per-chained-call": "off",
      "no-confusing-arrow": "off"
    }
  },
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
